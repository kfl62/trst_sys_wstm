- xhr_hidden_data({dialog: "show",oid: @object.id,js_ext: 'desk_cache'})
%form{action: "/sys/#{@path}",method: "get"}
  %table
    %thead
    %tbody
      %tr
        %td
          = haml :"#{@path}/_show"
    %tfoot
      - buttons = @current_user.admin? ? ['create','edit','delete','cancel'] : ['create','edit','cancel']
      - tr_buttonset(buttons,{td: {class: 'ta-ce'}})
